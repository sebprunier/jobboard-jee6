<?xml version="1.0"?>
<cloudbees-web-app xmlns="http://www.cloudbees.com/xml/webapp/1">
	<appid>sebprunier/jobboard-jee6</appid>
	<context-param>
		<param-name>application.environment</param-name>
		<param-value>development</param-value>
	</context-param>
	<resource name="jdbc/jobboard" auth="Container" type="javax.sql.DataSource">
		<param name="username" value="jobboard" />
		<param name="password" value="jobboard" />
		<param name="url" value="jdbc:cloudbees://jobboard" />
	</resource>
</cloudbees-web-app>